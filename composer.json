{
    "name": "knovators/laravel-model-caching",
    "description": "Automatic caching for Eloquent models.",
    "license": "MIT",
    "authors": [
        {
            "name": "Priyanka patel",
            "email": "priyankapatel@knovators.in"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "illuminate/cache": "^7.0",
        "illuminate/config": "^7.0",
        "illuminate/console": "^7.0",
        "illuminate/container": "^7.0",
        "illuminate/database": "^7.0",
        "illuminate/http": "^7.0",
        "illuminate/support": "^7.0",
        "predis/predis": "^1.1"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.9",
        "orchestra/testbench-browser-kit": "^5.0",
        "orchestra/testbench": "^5.0",
        "php-coveralls/php-coveralls" : "^2.2",
        "phpmd/phpmd": "^2.7",
        "phpunit/phpunit": "^8.0",
        "sebastian/phpcpd": "^5.0",
        "squizlabs/php_codesniffer": "^3.4",
        "symfony/thanks": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Knovators\\LaravelModelCaching\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/database/factories",
            "tests/database/seeds"
        ],
        "psr-4": {
            "Knovators\\LaravelModelCaching\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Knovators\\LaravelModelCaching\\Providers\\Service"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
